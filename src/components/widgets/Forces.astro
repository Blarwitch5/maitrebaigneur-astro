<Waves atTopOfSection />
<Container>
	<h2 class="forces__title">Pourquoi apprendre à nager avec Maître-Baigneur ?</h2>
	<div class="forces__list">
		{
			forces.map((force) => (
				<div class="force">
					<span class="force__icon" set:html={force.icon} />
					<h3 class="force__title">{force.name}</h3>
					<p class="force__txt">{force.txt}</p>
				</div>
			))
		}
	</div>
</Container>

<style lang="scss">
	.forces {
		&__title {
			text-align: left;
		}
		&__list {
			display: grid;
			grid-template-columns: repeat(3, 1fr);
			grid-gap: 2rem;
			transition: all 0.3s ease-in-out;

			@media screen and (max-width: 768px) {
				grid-template-columns: repeat(1, 1fr);
				grid-row: auto;
			}

			.force {
				color: var(--color-primary);
				display: grid;
				justify-content: center;
				align-content: flex-start;
				gap: 1rem;

				@media screen and (max-width: 768px) {
					row-gap: 0.5rem;
					grid-template-columns: 25% 1fr;
					grid-template-rows: repeat(2, min-content);
				}

				&__icon {
					display: block;
					width: 100px;
					height: 100px;
					margin: 0 auto;
					background-size: contain;
					background-repeat: no-repeat;
					background-position: center;

					@media screen and (max-width: 768px) {
						grid-row: 1 / span 2;
						display: grid;
						// align-items: center;
						width: calc(100% - 0.5rem);
						height: auto;
					}
				}

				&__title {
					margin: 0;
					text-align: center;
					font-size: clamp(1.6rem, 2vw + 1rem, 2rem);
					font-family: var(--ff-pacifico);

					@media screen and (max-width: 768px) {
						grid-row: 1 / span 1;
						text-align: left;
					}
				}
				&__txt {
					color: var(--color-primary);
					text-align: justify;

					@media screen and (max-width: 768px) {
						margin: 0;
						text-align: left;
					}
				}
			}
		}
	}
</style>
